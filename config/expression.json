{
    "constant": {
        "dot": ".", 
        "empty string": "", 
        "hd threshold": 720, 
        "playback aspect ration": 1.7777777777777777, 
        "space": " "
    }, 
    "expression": [
        {
            "definition": "^([0-9]{2}:[0-9]{2}:[0-9]{2},[0-9]{3}) --> ([0-9]{2}:[0-9]{2}:[0-9]{2},[0-9]{3})$", 
            "flags": 32, 
            "name": "srt time line"
        }, 
        {
            "definition": "([0-9]):([0-9]{2}):([0-9]{2}).([0-9]{2})", 
            "flags": 32, 
            "name": "ass timecode"
        }, 
        {
            "definition": "^Dialogue\\s*:\\s*(.*)$", 
            "flags": 32, 
            "name": "ass subtitle line"
        }, 
        {
            "definition": "^Format\\s*:\\s*(.*)$", 
            "flags": 32, 
            "name": "ass formation line"
        }, 
        {
            "definition": "(\\\\N)+", 
            "flags": 32, 
            "name": "ass condense line breaks"
        }, 
        {
            "definition": "\\{\\\\[^\\}]+\\}", 
            "flags": 32, 
            "name": "ass event command"
        }, 
        {
            "definition": "\\s+", 
            "flags": 32, 
            "name": "whitespace"
        }, 
        {
            "definition": "[\\/?<>:*|'\"^\\.]", 
            "flags": 32, 
            "name": "characters to exclude from filename"
        }, 
        {
            "definition": "[.!?]", 
            "flags": 32, 
            "name": "sentence end"
        }, 
        {
            "definition": "^[^/]+(?:\\s*/\\s*[^/]+)*$", 
            "flags": 32, 
            "name": "mediainfo value list"
        }, 
        {
            "definition": "\\||,", 
            "flags": 32, 
            "name": "tvdb list separators"
        }, 
        {
            "definition": "\\s*\\|\\s*", 
            "flags": 32, 
            "name": "space around tvdb list item"
        }, 
        {
            "definition": "\\s+/\\s+(?:\t)*", 
            "flags": 32, 
            "name": "clean xml"
        }, 
        {
            "definition": "yes|true|1", 
            "flags": 34, 
            "name": "true value"
        }, 
        {
            "definition": "(?:(?P<tzinfo>[A-Za-z/]+) )?(?P<year>[0-9]{4})(?:-(?P<month>[0-9]{2})(?:-(?P<day>[0-9]{2})(?:(?: |T)(?P<hour>[0-9]{2}):(?P<minute>[0-9]{2}):(?P<second>[0-9]{2}))?)?)?", 
            "flags": 32, 
            "name": "full utc datetime"
        }
    ]
}