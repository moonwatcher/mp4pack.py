{
    "system":{
        "domain":"portodelgato.no-ip.info",
        "host":"vito"
    },
    "repository":{
        "vito":{
            "domain":"portodelgato.no-ip.info",
            "temp":{
                "path":"/net/vito/volume/barcelona/temp"
            },
            "mongodb":{
                "database":"mp4pack",
                "host":"vito"
            },
            "volume":{
                "default":{
                    "index":true,
                    "scan":true
                },
                "synonym":["path"],
                "element":{
                    "pluto":{
                        "enable":false,
                        "name":"Pluto",
                        "path":"/net/vito/volume/kyoto/pluto"
                    },
                    "jupiter":{
                        "enable":false,
                        "name":"Jupiter",
                        "path":"/net/vito/volume/toronto/jupiter"
                    },
                    "saturn":{
                        "enable":false,
                        "name":"Saturn",
                        "path":"/net/vito/volume/paris/saturn"
                    },
                    "neptune":{
                        "enable":false,
                        "name":"Neptune",
                        "path":"/net/vito/volume/london/neptune"
                    },
                    "titan":{
                        "enable":false,
                        "name":"Titan",
                        "path":"/net/multivac/Volumes/hiroshima/titan"
                    },
                    "oberon":{
                        "enable":false,
                        "name":"Oberon",
                        "path":"/net/multivac/Volumes/valencia/oberon"
                    },
                    "eros":{
                        "enable":false,
                        "name":"Eros",
                        "path":"/net/multivac/Volumes/nagasaki/eros"
                    },
                    "pan":{
                        "enable":false,
                        "name":"Pan",
                        "path":"/Volumes/chicago/pan"
                    },
                    "atlas":{
