                    "apply":[
                        {"property":"preset", "value":"default"}
                    ]
                },
                {
                    "requires":["action", "kind"],
                    "equal":{"action":"transcode", "kind":"chp"},
                    "apply":[
                        {"property":"preset", "value":"default"}
                    ]
                },
                {
                    "requires":["action", "kind"],
                    "equal":{"action":"transcode", "kind":"ass"},
                    "apply":[
                        {"property":"preset", "value":"default"}
