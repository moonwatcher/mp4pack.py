                    "path":"/net/multivac/Volumes/valencia",
                    "alternate":[
                        "/net/multivac.lan/Volumes/valencia",
                        "/Volumes/valencia"
                    ]
                }
            ],
            "preset.default":[
                {
                    "requires":["action"],
                    "equal":{"action":"info"},
                    "apply":[
                        {"property":"preset", "value":"default"}
                    ]
                },
                {
                    "requires":["action"],
                    "equal":{"action":"explode"},
