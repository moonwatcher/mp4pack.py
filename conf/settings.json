                        "path":"/Volumes/huston/atlas"
                    },
                    "janus":{
                        "enable":false,
                        "name":"Janus",
                        "path":"/Volumes/columbia/janus"
                    }
                }
            },
            "mapping":[
                {
                    "enable":false,
                    "path":"/net/vito/volume/toronto",
                    "alternate":[
                        "/net/vito.lan/volume/toronto",
                        "/volume/toronto",
                        "/media/toronto",
                        "/Volumes/toronto"
                    ]
                },
                {
                    "enable":false,
                    "path":"/net/vito/volume/paris",
                    "alternate":[
                        "/net/vito.lan/volume/paris",
                        "/volume/paris",
                        "/media/paris",
                        "/Volumes/paris"
                    ]
                },
                {
                    "enable":false,
                    "path":"/net/vito/volume/london",
                    "alternate":[
                        "/net/vito.lan/volume/london",
